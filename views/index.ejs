<!DOCTYPE html>
<html>
  <%- include("./partials/head") %>
  <body>
    <header>
      <h1>
        Home Page of <%= name %>
      </h1>
    </header>
    <nav>
      <a href="/">Home</a>
      <a href="/about">About</a>
    </nav>
    <section>
      <p>
        Hello World! Welcome to the home page.
      </p>
      <% if (list.length > 0) { %>
      <ul>
        <% list.forEach( item => { %>
        <li><%= item %></li>
        <% }) %>
      </ul>
      <% } else { %>
      <p>There are no cars</p>
      <% } %>
    </section>
  </body>
</html>
