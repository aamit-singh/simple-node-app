{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "no-console": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off"
  }
}
